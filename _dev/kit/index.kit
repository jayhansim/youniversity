<script src="js/slick/slick.min.js"></script>
<script>
$(document).ready(function(){

  // articles carousels
  $('.articles-container').slick({
    slidesToShow: 4,
    slidesToScroll: 4,
    dots: true,
    prevArrow: '<a class="slick-arrow slick-arrow-prev">Prev</a>',
    nextArrow: '<a class="slick-arrow slick-arrow-next">Next</a>',
    responsive: [
    {
      breakpoint: 992,
      settings: {
        slidesToShow: 3,
        slidesToScroll: 3
      }
    },
    {
      breakpoint: 767,
      settings: {
        slidesToShow: 2,
        slidesToScroll: 2
